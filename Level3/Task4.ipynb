    "import time \n",
    "\n",
    "def decorator(func):\n",
    "    def wrapper():\n",
    "        start_time=time.time()\n",
    "        func()\n",
    "        end_time=time.time()\n",
    "        print(round(end_time-start_time))\n",
    "    return wrapper()\n",
    "@decorator  \n",
    "def sample():\n",
    "    sleep=time.sleep(1)\n"
